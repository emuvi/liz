{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'liz'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=liz"
                ],
                "filter": {
                    "name": "liz",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'liz'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=liz",
                    "--package=liz"
                ],
                "filter": {
                    "name": "liz",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'liz'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=liz",
                    "--package=liz"
                ],
                "filter": {
                    "name": "liz",
                    "kind": "bin"
                }
            },
            "args": ["lizs/test/hello"],
            "cwd": "${workspaceFolder}"
        },
    ]
}